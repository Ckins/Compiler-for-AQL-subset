create view Cap as
  extract regex /[A-Z]([a-z]*)/
    on D.text
    return group 0 as Cap
    and group 1 as Att
  from Document D;

create view Stt as
  extract regex /(Washing)ton|Georgia|Virginia/
    on D.text
    return group 0 as Stt
    and group 1 as Was
  from Document D;

output view Cap;
output view Stt;